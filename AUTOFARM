-- Create a ScreenGui to hold the GUI elements
local gui = Instance.new("ScreenGui")
gui.Parent = game.Players.LocalPlayer.PlayerGui

-- Create the main frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 100)
frame.Position = UDim2.new(0.5, -100, 0.5, -50)
frame.BackgroundColor3 = Color3.new(0, 0, 0) -- Initial background color
frame.BorderSizePixel = 0
frame.Parent = gui

-- Create the checkbox
local checkbox = Instance.new("TextButton")
checkbox.Size = UDim2.new(0, 20, 0, 20)
checkbox.Position = UDim2.new(0.1, 0, 0.5, -10)
checkbox.Text = ""
checkbox.Parent = frame

-- Create the checkmark
local checkmark = Instance.new("TextLabel")
checkmark.Size = UDim2.new(1, 0, 1, 0)
checkmark.BackgroundTransparency = 1
checkmark.Text = "âœ“"
checkmark.Font = Enum.Font.SourceSansBold
checkmark.TextSize = 18
checkmark.TextColor3 = Color3.new(0, 1, 0) -- Green color
checkmark.Visible = false
checkmark.Parent = checkbox

-- Checkbox toggle function
local isChecked = false
local function toggleCheckbox()
    isChecked = not isChecked
    checkmark.Visible = isChecked
    if isChecked then
        frame.BackgroundColor3 = Color3.new(0, 1, 0) -- Green background color
        -- Execute the provided code
        local code = 'loadstring(Game:HttpGet('https://raw.githubusercontent.com/123456789qwerty0/DarkHub/main/bp%20msgox'))()'
        loadstring(code)()
    else
        frame.BackgroundColor3 = Color3.new(0, 0, 0) -- Reset background color
    end
end

-- Connect the toggle function to the checkbox click event
checkbox.MouseButton1Click:Connect(toggleCheckbox)
